{
  "name": "vite-plugin-clean-pattern",
  "version": "1.0.2",
  "description": "A vite plugin to remove/clean your build folder(s) using regex patterns to match specific files.",
  "author": "Jason Hassold (https://github.com/JasonHassold)",
  "license": "MIT",
  "bugs": "https://github.com/JasonHassold/vite-plugin-clean-pattern/issues",
  "homepage": "https://github.com/JasonHassold/vite-plugin-clean-pattern",
  "repository": "https://github.com/JasonHassold/vite-plugin-clean-pattern.git",
  "keywords": [
    "vite-plugin",
    "vite",
    "clean",
    "remove",
    "directory",
    "files",
    "project-clean",
    "directory-removed",
    "vite-plugin-clean",
    "vite-plugin-clean-pattern"
  ],
  "main": "dist/index.js",
  "exports": {
    "import": "./exports/import.mjs",
    "require": "./exports/require.cjs"
  },
  "files": [
    "/dist",
    "/exports"
  ],
  "scripts": {
    "build": "bun build ./index.ts --outdir ./dist --format esm"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "vite": "^4.4.9"
  }
}